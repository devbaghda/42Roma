#include <string.h>
#include <stdio.h>

void ft_swap(char a, char b)
{
	char temp;
	
	temp = a;
	//printf("%s", a);
	a = b;
	b = temp;
}

void ft_sort_params(int argc, char **str)
{
    char *temp;
    int i, j;
    i = 1;
    //printf("%s, %s\n", str[1], str[2]);
    //iterate over every params(the one tio compare to)
    //printf("%s", str[0]);
    while(i < argc - 1)
    {
        //iterate over every other params
        j = i + 1;
        while(j < argc - i - 1)
        {
            if(strcmp(str[i], str[j]) > 0)
            {
                    //printf("%s", a);
                    //ft_swap(str[i], str[j]);
                	temp = str[i];
	                
	                str[i] = str[j];
	                str[j] = temp;
            }
            j++;
            
        }
        j = 0;
        i++;
    }
    
}
